package principal;

import java.util.ArrayList;

import processing.core.PApplet;
import processing.core.PVector;

public class Logica {
	
	private PApplet app;
	private ArrayList<Casilla> casillas;
	
	public Logica() {
		this.app = Main.app;
		this.casillas = new ArrayList<>();
		

		for(int i = 0 ; i < 3; i++) {
			Casilla c = new Casilla(new PVector((app.width/3)*i, (app.height/3)*i));
			casillas.add(c);
		}
		
	}
	
	
	public void pintar(){
		
	}


}


